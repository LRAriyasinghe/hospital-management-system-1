<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="700.0" prefWidth="1100.0"
            stylesheets="@../../../resources/styles/default/new_edit_window.css"
            xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="com.oasis.controller._A.NewWardController">
    <children>
        <GridPane layoutX="14.0" layoutY="14.0" prefHeight="388.0" prefWidth="479.0" AnchorPane.leftAnchor="50.0"
                  AnchorPane.topAnchor="50.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="142.0" minWidth="10.0" prefWidth="142.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="291.0" minWidth="10.0" prefWidth="252.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Name"/>
                <Label text="Description" GridPane.rowIndex="1"/>
                <Label text="Max patient count" GridPane.rowIndex="2"/>
                <AnchorPane GridPane.columnIndex="1">
                    <children>
                        <TextField fx:id="nameTextField" layoutY="22.0" prefWidth="200.0"/>
                    </children>
                </AnchorPane>
                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <children>
                        <TextArea fx:id="descriptionTextArea" prefHeight="78.0" prefWidth="250.0"/>
                    </children>
                </AnchorPane>
                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <children>
                        <TextField fx:id="maxPatientCountTextField" layoutY="27.0"/>
                    </children>
                </AnchorPane>
                <Label text="Gender acceptance" GridPane.rowIndex="3"/>
                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <children>
                        <ComboBox fx:id="genderAcceptanceComboBox" layoutY="26.0" prefHeight="25.0" prefWidth="150.0"/>
                    </children>
                </AnchorPane>
                <Label text="Supervisor" GridPane.rowIndex="4"/>
                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <children>
                        <TextField fx:id="supervisorTextField" layoutY="26.0" prefWidth="150.0"/>
                    </children>
                </AnchorPane>
            </children>
        </GridPane>
        <Button fx:id="okButton" layoutX="986.0" layoutY="651.0" mnemonicParsing="false" onAction="#okButtonOnAction"
                prefHeight="35.0" prefWidth="100.0" text="OK"/>
    </children>
</AnchorPane>
