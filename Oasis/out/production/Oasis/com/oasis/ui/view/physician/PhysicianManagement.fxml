                     <children>
                        <TextField fx:id="telephoneTextField" layoutY="11.0" prefWidth="150.0" />
                     </children>
                  </AnchorPane>
                  <AnchorPane GridPane.columnIndex="1">
                     <children>
                        <ComboBox fx:id="designationComboBox" layoutY="11.0" prefHeight="25.0" prefWidth="150.0" />
                     </children>
                  </AnchorPane>
               </children>
            </GridPane>
            <Button fx:id="saveButton" layoutX="854.0" layoutY="201.0" mnemonicParsing="false" onAction="#saveButtonOnAction" prefHeight="35.0" prefWidth="100.0" text="Save" AnchorPane.bottomAnchor="0.0" />
            <Button fx:id="newButton" layoutX="1000.0" layoutY="211.0" mnemonicParsing="false" onAction="#newButtonOnAction" prefHeight="35.0" prefWidth="100.0" text="New" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
            <Button fx:id="deleteButton" disable="true" layoutX="970.0" layoutY="143.0" mnemonicParsing="false" onAction="#deleteButtonOnAction" prefHeight="35.0" prefWidth="100.0" text="Delete" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
