            <HBox fx:id="searchPane" alignment="CENTER_LEFT" layoutY="3.0" prefHeight="45.0" prefWidth="350.0" spacing="20.0" AnchorPane.bottomAnchor="2.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="3.0">
               <children>
                  <Label text="Search" />
                  <HBox alignment="CENTER_LEFT" spacing="5.0">
                     <children>
                        <TextField fx:id="searchTextField" prefHeight="25.0" prefWidth="250.0" />
                        <Button fx:id="clearButton" mnemonicParsing="false" styleClass="mini-button" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="managePane" layoutY="50.0" prefHeight="400.0" prefWidth="1100.0">
         <children>
            <TableView fx:id="physicianTableView" layoutX="50.0" layoutY="50.0" prefHeight="410.0" prefWidth="1040.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
