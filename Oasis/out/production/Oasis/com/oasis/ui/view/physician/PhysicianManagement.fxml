<AnchorPane prefHeight="700.0" prefWidth="1100.0" styleClass="backgroundPane" stylesheets="@../../../resources/styles/default/management_edit_window.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.oasis.controller.physician.PhysicianManagementController">
   <children>
      <AnchorPane fx:id="navigationPane" prefHeight="50.0" prefWidth="1100.0">
